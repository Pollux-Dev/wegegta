{
  "name": "wegegta-portfolio",
  "description": "wegegta portfolio",
  "version": "1.0.0",
  "private": true,
  "author": "henzzo",
  "license": "MIT",
  "keywords": [
    "wegegta",
    "portfolio",
    "agriculture"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint-fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --quiet --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix --quiet",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "5.0.0-alpha.132",
    "@mui/material": "5.13.6",
    "@next/font": "13.4.7",
    "axios": "1.4.0",
    "clsx": "1.2.1",
    "filepond": "4.30.4",
    "filepond-plugin-file-validate-size": "2.2.8",
    "filepond-plugin-file-validate-type": "1.2.8",
    "filepond-plugin-image-preview": "4.6.11",
    "filepond-plugin-media-preview": "1.0.11",
    "formik": "2.4.2",
    "framer-motion": "^10.12.5",
    "gsap": "3.11.3",
    "lodash.debounce": "^4.0.8",
    "moment": "2.29.4",
    "next": "^13.4.7",
    "nprogress": "0.2.0",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-filepond": "^7.1.2",
    "react-hot-toast": "2.4.0",
    "react-markdown": "^4.2.2",
    "react-moment": "1.1.3",
    "sharp": "0.32.1",
    "simplebar-react": "3.2.4",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.11.9",
    "@types/nprogress": "0.2.0",
    "@types/qs": "6.9.7",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "sass": "1.56.1",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.0"
}
